<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/birudongker</item>
        <item name="colorPrimaryDark">@color/birudongker</item>
        <item name="colorAccent">@color/birudongker</item>
        <item name="android:statusBarColor">@color/background</item>
        <item name="android:windowLightStatusBar">true</item>
    </style>

    <!-- Base application theme. -->
    <style name="Theme.Fab_Bottom_app_bar" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">#FF8E11</item>
        <item name="colorPrimaryDark">#FF8E11</item>
        <item name="colorAccent">#000</item>
    </style>

    <style name="AlertDialog" parent="Base.Theme.AppCompat.Light.Dialog">
        <item name="android:textColor">#000000</item>
        <item name="android:textColorPrimary">#595959</item>
        <item name="android:colorAccent">#1b5e20</item>
    </style>

    <style name="LoginTextInputLayoutStyle" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="boxStrokeColor">@color/birudongker</item>
    </style>

</resources>
